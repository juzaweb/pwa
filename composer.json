{
    "name": "juzaweb/pwa",
    "description": "Progressive Web Application (PWA) for Juzaweb",
    "type": "juzaweb-module",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Juzaweb CMS",
            "email": "admin@juzaweb.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "juzaweb/core": "^5.0",
        "silviolleite/laravelpwa": "~2.0.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Modules\\Pwa\\": "src/",
            "Juzaweb\\Modules\\Pwa\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Modules\\Pwa\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
